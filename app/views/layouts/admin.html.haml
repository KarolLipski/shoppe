!!!
%html{:lang => "en"}
  = render 'layouts/admin/head'
  %body.nav-md
    .container.body
      .main_container
        = render 'layouts/admin/sidebar_left'
        / top navigation
        = render 'layouts/admin/top_nav'
        / /top navigation
        / page content
        .right_col{:role => "main"}
          %div
            .x_panel
              = yield
        / /page content
        / footer content
        %footer
          .pull-right
            Gentelella - Bootstrap Admin Template by
            %a{:href => "https://colorlib.com"} Colorlib
          .clearfix
        / /footer content
    = javascript_include_tag 'admin-bottom'

    = bootstrap_flash
    / Flot
    = yield :template_js